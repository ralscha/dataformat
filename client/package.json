{
  "name": "new-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "npx prettier --write ."
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": false,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.10.1",
  "dependencies": {
    "@angular/common": "21.2.0",
    "@angular/compiler": "21.2.0",
    "@angular/core": "21.2.0",
    "@angular/forms": "21.2.0",
    "@angular/platform-browser": "21.2.0",
    "@angular/router": "21.2.0",
    "@msgpack/msgpack": "3.1.3",
    "cbor-x": "1.6.0",
    "echarts": "6.0.0",
    "fast-xml-parser": "5.4.1",
    "flatbuffers": "25.9.23",
    "ngx-echarts": "21.0.0",
    "papaparse": "5.5.3",
    "protobufjs": "8.0.0",
    "rxjs": "7.8.2",
    "smile-js": "0.10.1",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular/build": "21.2.0",
    "@angular/cli": "21.2.0",
    "@angular/compiler-cli": "21.2.0",
    "@types/papaparse": "5.5.2",
    "prettier": "3.8.1",
    "typescript": "5.9.3"
  }
}
